}

[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests"
Test_Metadata_Directory="res://tests"
Tags=PoolStringArray(  )
Window_Size=Vector2( 1280, 720 )
Minimize_Window_When_Running_Tests=true
Port=6008
Run_All_Tests=Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":0,"unicode":0,"echo":false,"script":null)

Auto_Refresh_Tests=true
Display=8

[application]

config/name="wopeditor"
config/description="Words of Power Editor of TexnoMagic"
run/main_scene="res://ui/screens/abcs_screen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="WordsOfPower"
config/icon="res://icons/icon.png"
config/windows_native_icon="res://icons/icon.ico"
config/quit_on_go_back=false

[autoload]

Common="*res://texnomagic/common.gd"
Client="*res://texnomagic/client.gd"
WoPEditor="*res://wopeditor.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray(  )

[physics]

common/enable_pause_aware_picking=true

[rendering]

