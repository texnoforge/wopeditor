config_version=4

_global_script_classes=[ {
"base": "PopupPanel",
"class": "BaseDialog",
"language": "GDScript",
"path": "res://ui/dialog/base_dialog.gd"
}, {
"base": "Container",
"class": "HFlowContainer",
"language": "GDScript",
"path": "res://ui/h_flow_container/h_flow_container.gd"
}, {
"base": "Reference",
"class": "WAT",
"language": "GDScript",
"path": "res://addons/WAT/namespace.gd"
}, {
"base": "Node",
"class": "WATTest",
